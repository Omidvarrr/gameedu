<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>


<GridPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIController.RequestBarPage">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="250.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="barAnchorPane" maxHeight="70.0" maxWidth="700.0" minHeight="70.0" minWidth="700.0">
         <children>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="9.0" prefHeight="55.0" prefWidth="660.0" spacing="17.0">
               <children>
                  <JFXComboBox fx:id="requestComboBox" onAction="#paneConnectorToPage" prefHeight="26.0" prefWidth="266.0" />
                  <Label fx:id="label" text="درخواست خود را انتخاب کنید" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="anchorPane" minHeight="250.0" prefHeight="250.0" prefWidth="200.0" GridPane.rowIndex="1" />
   </children>
</GridPane>
